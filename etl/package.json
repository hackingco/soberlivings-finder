{
  "name": "soberlivings-etl",
  "version": "1.0.0",
  "description": "ETL Pipeline for FindTreatment.gov data to Supabase",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "etl:run": "tsx src/index.ts run",
    "etl:schedule": "tsx src/index.ts schedule",
    "etl:full-sync": "tsx src/index.ts full-sync",
    "etl:incremental": "tsx src/index.ts incremental",
    "etl:test": "tsx src/index.ts test",
    "etl:deploy": "npm run build && vercel --prod",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.0",
    "axios": "^1.6.0",
    "dotenv": "^16.4.0",
    "node-cron": "^3.0.3",
    "p-limit": "^3.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/node-cron": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "jest": "^29.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}